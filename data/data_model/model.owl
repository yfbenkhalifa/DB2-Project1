<http://www.db2poject.org/model/model.owl> rdf:type owl:Ontology .

#################################################################
#    Object Properties
#################################################################

###  http://www.db2poject.org/model/model.owl#consumes
:consumes rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty ;
          rdf:type owl:FunctionalProperty ;
          rdfs:domain :Resource ,
                      :Unit .


###  http://www.db2poject.org/model/model.owl#contains
:contains rdf:type owl:ObjectProperty ;
          owl:inverseOf :locatedIn ;
          rdf:type owl:FunctionalProperty ,
                   owl:InverseFunctionalProperty .


###  http://www.db2poject.org/model/model.owl#locatedIn
:locatedIn rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           rdf:type owl:FunctionalProperty ,
                    owl:InverseFunctionalProperty .


#################################################################
#    Data properties
#################################################################

###  http://www.db2poject.org/model/model.owl#private
:private rdf:type owl:DatatypeProperty ;
         rdfs:domain :Unit ;
         rdfs:range xsd:boolean ;
         rdfs:comment """Boolean value that specifies if the unit is a private or public property.
[TRUE => Private, FALSE => Public]""" .


###  http://www.db2poject.org/model/model.owl#quantity
:quantity rdf:type owl:DatatypeProperty ;
          rdfs:range xsd:double .


###  http://www.db2poject.org/model/model.owl#source
:source rdf:type owl:DatatypeProperty ;
        rdfs:subPropertyOf owl:topDataProperty ;
        rdfs:domain :Resource ;
        rdfs:range xsd:string ;
        rdfs:comment """This property specifies the source of the resource. 
i.e. Electricity, Petrol, Solar ecc..""" .


#################################################################
#    Classes
#################################################################

###  http://www.db2poject.org/model/model.owl#Continent
:Continent rdf:type owl:Class .


###  http://www.db2poject.org/model/model.owl#Country
:Country rdf:type owl:Class .


###  http://www.db2poject.org/model/model.owl#Location
:Location rdf:type owl:Class .


###  http://www.db2poject.org/model/model.owl#Mobile
:Mobile rdf:type owl:Class ;
        rdfs:subClassOf :Unit .


###  http://www.db2poject.org/model/model.owl#RealEstate
:RealEstate rdf:type owl:Class ;
            rdfs:subClassOf :Unit .


###  http://www.db2poject.org/model/model.owl#Resource
:Resource rdf:type owl:Class .


###  http://www.db2poject.org/model/model.owl#Unit
:Unit rdf:type owl:Class ;
      rdfs:comment """Identifies a property that consumes/produces resources.
i.e. Car, Plane, House, Train Station..""" .


#################################################################
#    Individuals
#################################################################

###  http://www.db2poject.org/model/model.owl#Europe
:Europe rdf:type owl:NamedIndividual ,
                 :Continent .


###  http://www.db2poject.org/model/model.owl#Italy
:Italy rdf:type owl:NamedIndividual ,
                :Mobile ;
       :locatedIn :Italy .


###  http://www.db2poject.org/model/model.owl#petrol
:petrol rdf:type owl:NamedIndividual ,
                 :Resource ;
        :consumes :petrol ;
        :quantity 10 .


###  http://www.db2poject.org/model/model.owl#testUnit
:testUnit rdf:type owl:NamedIndividual ,
                   :Mobile ;
          :consumes :petrol .


